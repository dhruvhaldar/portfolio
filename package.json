{
  "name": "@once-ui-system/magic-portfolio",
  "version": "1.3.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm process-images && next build",
    "start": "serve out",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "build:prod": "cross-env NODE_ENV=production pnpm process-images && next build",
    "build:analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "benchmark": "node scripts/benchmark.mjs",
    "process-images": "node scripts/process-images.js",
    "test": "vitest src",
    "test:watch": "vitest src",
    "test:coverage": "vitest run --coverage src"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.2",
    "@mdx-js/loader": "^3.1.0",
    "@next/mdx": "^16.0.10",
    "@next/third-parties": "^16.1.1",
    "@once-ui-system/core": "^1.5.6",
    "classnames": "^2.5.1",
    "cookie": "^1.0.2",
    "escape-html": "^1.0.3",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.27",
    "lazyframe": "^2.2.7",
    "next": "16.0.10",
    "next-mdx-remote": "^5.0.0",
    "prismjs": "^1.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-masonry-css": "^1.0.16",
    "react-pdf": "^9.2.1",
    "rehype-katex": "^7.0.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "remark-math": "^6.0.0",
    "sass": "^1.86.3",
    "sharp": "^0.34.5",
    "transliteration": "^2.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@csstools/postcss-global-data": "^2.1.1",
    "@next/bundle-analyzer": "^16.1.1",
    "@next/eslint-plugin-next": "16.0.10",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.17.30",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.5",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "chrome-launcher": "^1.2.1",
    "critters": "^0.0.25",
    "cross-env": "^10.1.0",
    "eslint": "^9.25.0",
    "eslint-config-next": "16.0.10",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.3.0",
    "lighthouse": "^13.0.1",
    "once-ui-cli": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-custom-media": "^10.0.8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^9.6.0",
    "react-devtools": "^6.1.1",
    "serve": "^14.2.5",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "tzdata": "^1.0.44",
    "vitest": "^4.0.16"
  },
  "serve": {
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "Referrer-Policy",
            "value": "strict-origin-when-cross-origin"
          },
          {
            "key": "Permissions-Policy",
            "value": "camera=(), microphone=(), geolocation=()"
          },
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://www.googletagmanager.com; style-src 'self' 'unsafe-inline'; img-src 'self' blob: data: https://www.google-analytics.com https://*.ytimg.com; font-src 'self'; object-src 'none'; base-uri 'self'; form-action 'self' https://app.kit.com; frame-ancestors 'none'; connect-src 'self' https://www.google-analytics.com https://*.googleapis.com https://*.youtube.com; media-src 'self' https://*.youtube-nocookie.com; frame-src 'self' https://*.youtube-nocookie.com; upgrade-insecure-requests;"
          },
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=63072000; includeSubDomains; preload"
          }
        ]
      }
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "sharp",
      "canvas"
    ]
  },
  "resolutions": {
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "inflight": "^2.0.0",
    "boolean": "^3.2.0"
  },
  "description": "Dhruv Haldar Portfolio",
  "author": "Dhruv Haldar",
  "browserslist": [
    "chrome >= 87",
    "firefox >= 78",
    "edge >= 88",
    "safari >= 14"
  ]
}
